<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="icon" format="reference" />
    <attr name="color" format="color" />
    <attr name="text" format="string" />
    <attr name="text_size" format="dimension" />

    <declare-styleable name="ChangeColorIconView">
        <attr name="icon" />
        <attr name="color" />
        <attr name="text" />
        <attr name="text_size" />
    </declare-styleable>


    <declare-styleable name="SpringView">
        <attr name="type">
            <enum name="overlap" value="0"/>
            <enum name="follow" value="1"/>
        </attr>
        <attr name="give">
            <enum name="both" value="0"/>
            <enum name="top" value="1"/>
            <enum name="bottom" value="2"/>
            <enum name="none" value="3"/>
        </attr>
        <attr name="header" format="reference"/>
        <attr name="footer" format="reference"/>
    </declare-styleable>


    <declare-styleable name="CircularReveal">
        <attr name="startRadius" format="dimension" />
        <attr name="endRadius" format="dimension" />
        <attr name="centerOn" format="reference" />
    </declare-styleable>
    <declare-styleable name="StartAnimatable">
        <attr name="android:src" />
    </declare-styleable>

</resources>